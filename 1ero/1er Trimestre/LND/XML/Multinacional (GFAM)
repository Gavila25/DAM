	<!ELEMENT bonos (precios+, paisProducAnt+, estable?, paisDeProcedencia+, valorDeseado+, valorMinimo+, valorMinimo+)>
		<!ELEMENT precios(#PCDATA)>
		<!ELEMENT paisProducAnt (#PCDATA)>
		<!ELEMENT estable (#PCDATA)>
		<!ELEMENT paisDeProcedencia (#PCDATA)>
		<!ELEMENT valorDeseado (#PCDATA)>
		<!ELEMENT valorMinimo (#PCDATA)>
		<!ELEMENT valorMaximo (#PCDATA)>

	<!ELEMENT letras (interesPagadero+, paisProducAnt+, precios+,paisEmisor+)>
		<!ELEMENT interesPagadero (#PCDATA)>
		<!ELEMENT paisProducAnt(#PCDATA)>
		<!ELEMENT precios (#PCDATA)>
		<!ELEMENT paisEmisor (#PCDATA)>

]>




