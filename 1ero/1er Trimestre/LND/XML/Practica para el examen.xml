<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<xsi:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema">
    <xsi:element name="pagina"/>
    <xsi:complextype>
        <xsi:sequence>
            <xsi:element name="nombre" type="xsi:string"/>
            <xsi:element name="descripcion" type="xsi:string"/>
            <xsi:element name="url" type="xsi:anyURL"/>
        </xsi:sequence>

        <xsi:element name="marcadores">
            <xsi:complextype>
                <xsi:sequence>
                    <xsi:element ref="pagina" maxOcurrs="unbounded"/>
                </xsi:sequence>
            </xsi:complextype>
        </xsi:element>
    </xsi:complextype>
</xsi:schema>

